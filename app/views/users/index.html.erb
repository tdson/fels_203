<% provide :title, t(".title") %>
<div class="wrapper">
  <div class="col-md-6 col-md-offset-3">
    <%= form_tag users_path, method: :get, id: "admin_search",
      class: "row" do %>
      <%= render "shared/search_form" %>
    <% end %>
  </div>
  <div class="col-md-6 col-md-offset-3">
    <div class="panel panel-default sidebar-menu">
      <div class="panel-heading">
        <h3 class="panel-title"><%= t ".header" %></h3>
      </div>
      <div class="panel-body">
        <ul class="users" id="result_search">
          <%= render @users %>
        </ul>
      </div>
      <div class="panel-footer text-center" id="paginations">
        <%= will_paginate %>
      </div>
    </div>
  </div>
</div>
